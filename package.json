{
  "name": "reactnativeaccessibilityproperties",
  "displayName": "ReactNativeAccessibilityProperties",
  "description": "Automatically provides accessibility properties to React Native components",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reactnativeaccessibilityproperties.addAccessibilityProperties",
        "title": " Adds Accessibility Properties to React Native Core Components"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@babel/generator": "^7.22.3",
    "@babel/parser": "^7.22.4",
    "@babel/traverse": "^7.22.4",
    "@babel/types": "^7.22.4",
    "@types/babel__generator": "^7.6.4",
    "@types/babel__traverse": "^7.20.1"
  }
}
